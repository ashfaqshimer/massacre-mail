		"mongoose": "^5.6.11",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^2.0.0",
		"path-parser": "^4.2.0",
		"redux-form": "^8.2.6",
		"sendgrid": "^5.2.3",
		"stripe": "^7.8.0"
	},
